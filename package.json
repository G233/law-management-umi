{
  "private": true,
  "name": "law-management-umi",
  "scripts": {
    "start": "w2 add .whistle.js --force && max dev",
    "build": "npm run fnBuild &&  npm run fnDeployPrd  && max build ",
    "postinstall": "max setup",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "fnBuild": "esbuild functions/src/index.ts --bundle --outdir=functions/fn_call  --platform=node --target=node10.4 --minify  ",
    "fnDeployDev": " npm run fnBuild  && tcb fn deploy fn_call --force --config-file cloudbaserc.dev.json ",
    "fnDeployPrd": " npm run fnBuild && tcb fn deploy fn_call --force --config-file cloudbaserc.prd.json"
  },
  "engines": {
    "node": "16",
    "pnpm": ">=7.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.6",
    "@ant-design/pro-card": "^2.6.0",
    "@ant-design/pro-form": "^2.25.1",
    "@ant-design/pro-layout": "^7.19.0",
    "@ant-design/pro-table": "^3.15.2",
    "@babel/runtime": "^7.14.0",
    "@cloudbase/js-sdk": "^1.5.0",
    "@cloudbase/node-sdk": "^2.5.2",
    "@umijs/max": "^4.1.8",
    "ahooks": "^2.10.5",
    "antd": "^5.16.1",
    "classnames": "^2.3.1",
    "moment": "^2.29.1",
    "rc-util": "^5.13.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.0.9",
    "uuid": "^8.3.2",
    "wx-server-sdk": "^2.4.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.3.1",
    "@types/node": "^18.19.31",
    "@types/react": "^18.2.75",
    "@types/react-dom": "^18.2.24",
    "esbuild": "^0.11.23",
    "eslint-plugin-react-hooks": "^4.6.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.3.1",
    "typescript": "^4.3.2",
    "yorkie": "^2.0.0"
  }
}